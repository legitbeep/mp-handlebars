                                <tr>
                                    <td align="center" class="social-icons">
                                        {{#if socialLinks}}
                                            {{#each socialLinks}}
                                            <a href="{{url}}" style="display: inline-block; margin: 0 8px;">
                                                <img src="{{icon}}" alt="{{platform}}" width="32" height="32" style="display: block;">
