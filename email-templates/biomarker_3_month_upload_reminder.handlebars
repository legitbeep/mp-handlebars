                            <div style="margin:20px 0 0px 0;">
                                <table role="presentation" cellspacing="0" cellpadding="0" border="0" width="100%">
                                    <tr>
                                        <td align="start" style="padding: 0;">
                                            <table role="presentation" cellspacing="0" cellpadding="0" border="0" >
                                                <tr >
                                                    <td >
                                                        <a href="{{uploadReportUrl}}" style="background-color: #017F5C; border-radius: 25px; display: inline-block; padding: 8px 16px; font-family: 'SF Pro Text', Arial, sans-serif; font-size: 12px; font-weight: 600; color: #ffffff !important; text-decoration: none; border-radius: 25px;margin-right:10px;">
