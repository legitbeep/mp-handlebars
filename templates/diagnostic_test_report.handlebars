  </section>
  <section style="padding: 0 20px;">
    <div style="border-bottom: 1px solid #3939391b; display:flex; justify-content:space-between; align-items:center; padding: 12px 0; margin-top:8px;">
      <h2 class="header-text2">
        Your Score
      </h2>
      <span>
        <span class="body-text2" style="color:#16A9B1;">Symptom</span>
        <span class="body-text2">{{result.score_title}}</span>
      </span>
    </div>
    {{!-- TODO: change icon --}}
    {{!-- <h2 style="text-align:center;margin: 0.9rem 0 1.5rem 0; display:flex; align-items:center; gap:1rem; justify-content: center;">
      <img src="https://cdn.mindpeers.co/dark-mode/tools/diagnostic-test/report-icon.png" alt="icon" style="height: 1.5rem; width: auto;object-fit:contain;"  /> 
      Report Card
    </h2> --}}
    {{#each report}}
      {{#ifType this 'undefined'}}
      {{else}}
        {{#switch this.type}}
          {{#case 'GRAPH_SECTION'}}
            <div style="width:100%;">
              {{#each this.cards}}
                <div style="width:100%;">
                {{#switch this.type}}
                  {{#case "BAR"}}
                    <div style="display:flex;flex-direction:column; gap:18px; margin:24px 0;">
                      {{#each this.data}}
                        <div class="bar-graph">
                          <span class="primary-cta-text" style="width:80px;">{{this.name}}</span>
                          <div style="display:flex; justify-content:space-between; align-items:center; flex:1;">
                            {{!-- create this.value color squares of 16px and remaining this.max_value in #A5A5A5 --}}
                            {{#numberLoop this.max_value}}
                              <div style="height:16px; width:16px;">
                                {{#compareNum number '<' ../this.value}}
                                  <div style="height:16px; width:16px; background-color: {{parent.color}};"></div>
                                {{/compareNum}}
                                {{#compareNum number '>=' ../this.value}}
                                  <div style="height:16px; width:16px; background-color: #A5A5A5;"></div>
                                {{/compareNum}}
                              </div>
                            {{/numberLoop}}
                          </div>
                          <div style="width:62px">
                            <span class="primary-cta-text">{{this.value}}</span>
                            <span class="primary-cta-text" style="color:#090A0B99;">/{{this.max_value}}</span>
                          </div>
                        </div>
                      {{/each}}
                      <div style="background: #F5FBFA;border: 1px solid #EBF3F2;border-radius: 20px;padding:12px 20px;align-self: flex-end;">
                        {{this.description}}
                      </div>
                    </div>
                  {{/case}}
                  {{#case "METER"}}
                    {{#with this}}
                      <div class="srrs-score-bar">
                        <div class="score-bar">
                          <div class="score-segment score-segment-1"></div>
                          <div class="score-segment score-segment-2"></div>
                          <div class="score-segment score-segment-3"></div>
                          <div class="score-segment score-segment-4"></div>
                          <div class="score-segment score-segment-5"></div>
                          <div class="score-segment score-segment-6"></div>
                        </div>
                        <div class="score-value header-text5" style="left: {{getPointerPosition this.value this.max_value}}%;">{{this.value}}</div>
                      </div>
                    {{/with}}
                    <div style="display:flex; align-items:center; justify-content:center;margin-top:12px;">
                      <h2 class="subtitle-text1" style="color:#090A0B99;">{{this.title}}</h2>
                    </div>
                  {{/case}}
                {{/switch}}
            </div>
              {{/each}}
            </div>
          {{/case}}
          {{#case 'QUESTIONS_SECTION'}}
            <div style="width:100%;">
              {{#each this.cards}}
              <div style="width:100%; display:flex; gap:1.5rem; align-items:center; margin: 36px 0;" class="dont-break">
                <div>
                  <h2 class="header-text4">{{this.heading}}</h2>
                  <span class="body-text2">
                  {{{formatDescription this.description}}}
                  </span>
                </div>
                <img src={{this.image}} alt="icon" style="height: 8rem; width: auto;object-fit:contain;"  />
              </div>
              {{/each}}
            </div>
          {{/case}}
          {{#case "WIDE_SECTION"}}
              {{#each this.cards}}
                <section class="quote-section dont-break" style="position:relative;margin-bottom: 2rem;">
                   <h2 style="z-index:1;" class="header-text4">{{this.heading}}</h2>
                   <div style="display:flex; gap:0.85rem;">
                    {{#each this.tags}}
                      <div style="background: #FFFFFF;border: 1px solid #EBF3F2;border-radius: 20px;padding:8px 20px;" class="body-text1">{{this}}</div>
                    {{/each}}
                   </div>
                </section>
              {{/each}}
          {{/case}}
          {{#case "IMAGE_BOX_TEXT"}}
            <section class="dont-break flex-col-center" style="margin: 1rem 0; z-index:1;">
              {{#each this.cards}}
                <div style="display:flex; 0; align-items:center; margin: 36px 0; background:#FFFFFF;position:relative">
                  <img src="https://cdn.mindpeers.co/dark-mode/tools/diagnostic-test/report/peeking-person.svg" alt="icon" style="height: 100%; width: auto;object-fit:contain;position:absolute;inset:0;z-index:1;"  />
                  <div style="border: 1px solid #6B6C6D;border-left:none;background:#fff; padding: 1rem;z-index:1;width:100%;margin-left:3rem;">
                    <h2 class="header-text2" style="color:#16A693;margin-bottom:1rem;">{{this.heading}}</h2>
                    <p class="body-text2" style="color:#16A693; margin:0;">{{this.description}}</p>
                  </div>
                </div>
              {{/each}}
            </section>
          {{/case}}
          {{#case "BG_IMAGE_SECTION"}}
            <section class="dont-break" style="margin-bottom:2rem;">
                <h1 class="header-text4">{{this.heading}}</h1> 
                <div class="graph-bg" style="background: url('{{this.background}}') no-repeat;min-height:22rem;background-position: center;display:flex;align-items:center;justify-content:center;">
                  {{#ifType this.cards 'object'}}
                    {{#each this.cards}}
                        {{#ifcond (mod @index 2) 0}}
                        <div style="flex:0.5;max-width:70%;padding:0.85rem 0.5rem;background: #FAFAFB;border-radius: 20px;align-self: flex-start;">
                          <span class="body-text1">{{this}}</span>
                        </div>
                        {{/ifcond}}
                        {{#ifcond (mod @index 2) 1}}
                        <div style="flex:0.5;max-width:70%;padding:0.85rem 0.5rem;background: #FAFAFB;border-radius: 20px;align-self: flex-end;">
                          <span class="body-text1">{{this}}</span>
                        </div>
                        {{/ifcond}}
                    {{/each}}
                  {{else}}
                    <div style="display:flex;align-items:center;flex-direction:column;">
                      <div style="position:relative;display:flex;align-items:center;flex-direction:column;height:130px;width:240px;padding:20px;">
                        <img src="../assets/question-bubble.png" style="position:absolute;inset:0;z-index:1;height: 200px;width: 400px;left: -50px;top: -20px;" />
                        <img src="{{this.icon}}" alt="icon" style="height:1.25rem; width: auto;object-fit:contain;z-index:2;transform:rotate(-10deg);"  />
                        <h2 class="subtitle-text2" style="color:#090A0B;z-index:2;text-align:center;">{{this.question}}</h2>
                      </div>
                      <div style="display:flex;width: 600px;justify-content:space-between;margin-top:-2rem;">
                        {{#each this.answers}}
                        <div style="height:200px;width:auto;max-width:150px;">
                          <img src="../assets/arrow-doodle.png" class="{{#ifcond @index 0}}first-arrow{{/ifcond}}{{#ifcond @index 1}}second-arrow{{/ifcond}}{{#ifcond @index 2}}third-arrow{{/ifcond}}" />
                          <div style="position:relative;display:flex;align-items:center;justify-content:center;"> 
                            <img src="https://cdn.mindpeers.co/dark-mode/tools/diagnostic-test/report/ans-bubble.svg" style="position:absolute;max-width:150%;z-index:1;" />
                            <span class="body-text1" style="z-index:2;color:#16A693;padding:10px 18px;text-align:center;word-wrap: break-word;">{{this}}</span>
                          </div>
                        </div>
                      {{/each}}
                      </div>
                    </div>
                  {{/ifType}}
                </div>
            </section>
          {{/case}}
          {{#case "PARALLEL_CARDS_SECTION"}}
            <section class="dont-break" style="margin: 4rem 0;">
              <h2 style="text-align:start;margin:1rem 0;">{{this.heading}}</h2>
              <h3 style="text-align:start;margin:1rem 0;color:#090A0B99;" class="body-text1">{{this.sub_heading}}</h3>
              <div style="display:flex; gap:1.25rem; border-radius:0.5rem;">
                {{#each this.cards}}
                  {{#switch this.type}}
                    {{#case "FILLED_CARD"}}
                      <div class="item-box" style="flex:0.5; padding:1rem; display:flex; flex-direction:column; gap:1rem;background:{{this.background}}; align-items:center;">
                        {{#ifType this.header_color "string"}}
                            <h1 class="subtitle-text2" style="color:{{this.header_color}};margin-bottom:0.5rem;">{{this.heading}}</h1>                            
                        {{/ifType}}
                        {{#ifType this.image "string"}}
                          <img src={{this.image}} style="width:100%; max-width:15rem; height:auto; margin:0 auto;" alt="" />
                        {{/ifType}}
                        <span class="caption-text" style="color:#090A0B99;">{{this.description}}</span>
                    </div>
                    {{/case}}
                    {{#case "BG_IMAGE_CARD"}}
                      <div class="item-box" style="flex:0.5; padding:1rem; display:flex; flex-direction:column; gap:0.8rem;;position:relative">
                          {{#ifType this.header_color "string"}}
                              <h1 class="subtitle-text1" style="color:{{this.header_color}};margin-bottom:0.5rem;z-index:1;">{{this.heading}}</h1>                            
                          {{/ifType}}
                          {{#ifType this.image "string"}}
                            <img src={{this.image}} style="width:100%; max-width:15rem; height:auto; max-height:90%;object-fit:contain; margin:0 auto;position:absolute;right:0;bottom:0;z-index:0;" alt="" />
                          {{/ifType}}
                          <span class="body-text2" style="color:#090A0B66;z-index:1;">{{this.description}}</span>
                      </div>
                    {{/case}}
                    {{#case "POINTER_CARD"}}
                      <div class="item-box" style="flex:0.5; padding:1rem; display:flex; flex-direction:column; gap:0;">
                        {{#ifType this.header_color "string"}}
                          <div style="display:flex;flex-direction:column;margin-bottom: 1rem;">
                            <h1 class="subtitle-text2" style="color:{{this.header_color}};margin-bottom:0.5rem;">{{this.heading}}</h1>
                            <span class="body-text3" style="color:#090A0BCC;">{{this.description}}</span>
                          </div>
                        {{/ifType}}
                        {{#ifType this.image "string"}}
                          <img src={{this.image}} style="width:100%; max-width:15rem; height:auto; margin:0 auto;" alt="" />
                        {{/ifType}}
                        <span class="body-text3" style="color:#090A0BCC;margin: 1rem 0;">{{this.pointers_title}}</span>
                        <ul  class="body-text3" style="color:#090A0BCC;display:flex; flex-direction:column; gap:0.5rem;padding:0;margin-top:0;">
                          {{#each this.pointers}}
                            <li class="body-text3" style="color:#090A0B;list-style-type: none; display:flex; gap:0.5rem; ">
                              <img src="https://cdn.mindpeers.co/dark-mode/tools/diagnostic-test/report/pin-icon.png" style="height: 14px; width:auto;" alt="" />
                              <span>{{this}}</span>  
                            </li>
                          {{/each}}
                        </ul>
                    </div>
                    {{/case}}
                  {{/switch}}
                {{/each}}
              </div>
            </section>
          {{/case}}
          
          {{#case "FOOTER_SECTION"}}
            <footer class="footer dont-break no-padding" style="margin-top: 1rem;">
                <div style="display: flex; padding: 2rem 4rem; gap:0.5rem;background:#F5FBFA;">
                  <div>
                    <h2 style="margin:0;">{{this.heading}}</h2>
                    <p style="margin:0;">{{this.description}}</p>
                    <p style="white-space:pre-line; margin:1.25rem 0 1rem 0;">With love,</p>
                    <img src="https://cdn.mindpeers.co/website-images/mp-logo.png" alt="logo" style="height: 2rem; width: auto; display: block;" />  
                  </div>
                </div>
                <div style="display:flex;border: 1px solid #393939;border-left:none; border-right:none;">
                  {{#each this.cards}}
                    <div style="padding:0.75rem 0; flex:0.5; display:flex; justify-content:center;align-items:center; gap:1rem;border-right:{{#ifcond @index 0}}1px solid #393939;{{else}}none;{{/ifcond}};">
                      {{!-- <img src={{this.image}} alt="image" style="height:5rem; width:5rem; object-fit: contain; display: block;" /> --}}
                      <h2 class="secondary-cta-text" style="color:#16A9B1;">{{this.heading}}</h2>
                    </div>
                  {{/each}}
                </div>
                <div style="padding: 0.5rem 0; display:flex; justify-content:center;">
                  <span style="margin:0.5 auto; text-align:center; color:#090A0B66;" class="caption-text">This is MindPeers India Private Limited Copyright {{currentYear}}. Please do not circulate or modify.</span>
                </div>
            </footer>
          {{/case}}
          {{#default}}
            <!-- Default code -->
          {{/default}}
        {{/switch}}
      {{/ifType}}
    {{/each}}
  </section>
  
  </div>
</body>

</html>