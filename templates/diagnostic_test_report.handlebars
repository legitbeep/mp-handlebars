.srrs-score-bar {
  display: flex;
  align-items: flex-end;
  width: calc(100% - 4rem);
  position: relative;
  height:7.5rem;
  margin: 0 auto;
}

.score-bar {
  display: flex;
  width: 100%;
  height: 20px;
  border-radius: 20px;
  overflow: hidden;
  z-index: 0;
}

.score-segment {
  flex: 1;
}

.score-segment-1 { background-color: #1B9004; }
.score-segment-2 { background-color: #22B004; }
.score-segment-3 { background-color: #FFCC00; }
.score-segment-4 { background-color: #FF9934; }
.score-segment-5 { background-color: #CD3301; }
.score-segment-6 { background-color: #9A2D28; }

.score-pointer-container {
  position: absolute;
  height: 100%;
}

.score-pointer {
  position: absolute;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 30px;
  height: 30px;
  background-color: #333;
  border-radius: 50%;
  transform: translateX(-50%);
}

.score-value {
  color: #090A0B;
  font-weight: bold;
  position: absolute;
  top: 0;
  height: 3.75rem;
  width: 3.75rem;
  border-radius: 50%;
  background-color: #E0E0E0;
  border: 4px solid #FAFAFB;
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 2;
}
.score-value::after {
  content: '';
  position: absolute;
  bottom: -37px;
  left: 50%;
  transform: translateX(-50%);
  width: 0;
  height: 0;
  border-left: 10px solid transparent;
  border-right: 10px solid transparent;
  border-top: 37px solid #E0E0E0;
  border-bottom: none;
  z-index: 1;
}
.answer-circle{
  position: relative;
}
.first-arrow{
  height:4rem;
  margin-bottom: 1rem;
  transform: scaleX(-1);
}
.second-arrow{
  height:7.5rem;
  margin-bottom: 1rem;
}
.third-arrow{
  height:4rem;
  margin-bottom: 1rem;
}
